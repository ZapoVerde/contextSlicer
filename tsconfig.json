{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"]
  },
  "include": ["src"]
}